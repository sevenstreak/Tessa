import requests
from dotenv import load_dotenv
import os	
import json


load_dotenv()

key = "?key=" + os.getenv('RAWG_KEY')

url = "https://api.rawg.io/api/games/" + "dishonored" + key                 
# "?key=" + os.getenv('RAWG_KEY') 

""" headers = {
	
	"X-RapidAPI-Host": "rawg-video-games-database.p.rapidapi.com"
}
 """
response = requests.request("GET", url)
decoded = response.json()
#json = json.load(response).read()


print(decoded['metacritic'])
