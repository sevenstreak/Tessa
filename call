import requests
from dotenv import load_dotenv
import os

load_dotenv()

url = "https://api.rawg.io/api/games//movies/?key=" + os.getenv('RAWG_KEY') 

""" headers = {
	
	"X-RapidAPI-Host": "rawg-video-games-database.p.rapidapi.com"
}
 """
response = requests.request("GET", url)

print(response.text)
